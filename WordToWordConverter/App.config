<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="algorithmsection" type="WordToWordConverter.Configuration.AlgorithmSettingsSection, WordToWordConverter, Culture=neutral, PublicKeyToken=null" />
    <section name="dictionarysection" type="WordToWordConverter.Configuration.DictionarySettingsSection, WordToWordConverter, Culture=neutral, PublicKeyToken=null" />
  </configSections>
  <algorithmsection maxSteps="50" maxPopulation="10" />
  <dictionarysection isDatabase="true" fileName="..\..\dic1-20.txt" needSort="true" connectionString="dicDB" />
  
  <connectionStrings>
    <clear />
    <add name="dicDB" connectionString="data source=data.db" providerName="System.Data.SqLite" />
  </connectionStrings>
  
  <system.data>    
    <DbProviderFactories>
      <remove invariant="System.Data.SQLite" />
      <add name="SQLite Data Provider" invariant="System.Data.SQLite" description=".Net Framework Data Provider for SQLite" type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite" />
    </DbProviderFactories>
  </system.data>
    
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
</configuration>